<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Route">
        <property name="route" type="string" />
        <property name="parameters" type="array"/>
    </transfer>

    <transfer name="ResourceShare">
        <property name="idResourceShare" type="int" />
        <property name="customerReference" type="string" />
        <property name="uuid" type="string" />
        <property name="resourceType" type="string" />
        <property name="resourceData" type="string" />
        <property name="expiryDate" type="string" />
    </transfer>

    <transfer name="Message">
        <property name="value" type="string" />
    </transfer>

</transfers>
